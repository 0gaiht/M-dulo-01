programa
{
	
	funcao inicio()
	{
		inteiro contador = 0
		cadeia clientes [][] = {{"João","(SP)","(11)91999"},{"Pedro","(RJ)","(11)888"},{"Maria","(CE)","(91)777"}}

		faca{
			escreva ("Fornecedor:" + clientes[contador][0] + " - Estado:" + clientes[contador][1] + " - Fone:" + clientes[contador][2] + "\n")
			contador++
		}enquanto (contador<=2) 
	}
}





